{
	"name": "diecoding/yii2-slick",
	"description": "The last carousel you'll ever need for Yii2",
	"authors": [
		{
			"name": "Sugeng Sulistiyawan",
			"email": "sugeng.sulistiyawan@gmail.com"
		}
	],
	"type": "yii2-widget",
	"keywords": [
		"yii2",
		"widget",
		"slick",
		"carousel",
		"jquery"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/sugeng-sulistiyawan/yii2-slick/issues",
		"source": "https://github.com/sugeng-sulistiyawan/yii2-slick"
	},
	"require": {
		"php": ">=7.4.0",
		"yiisoft/yii2": "~2.0.0",
		"npm-asset/slick-carousel": "~1.8.1"
	},
	"require-dev": {
		"phpunit/phpunit": "~9.5.0"
	},
	"autoload": {
		"psr-4": {
			"diecoding\\yii2\\slick\\": "src/"
		}
	},
    "autoload-dev": {
        "psr-4": {
            "diecoding\\yii2\\slick\\tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
